{
  "expo": {
    "name": "DangStore",
    "slug": "dangstore",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/DANGSTORELOGOPRUEBA__1.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/DANGSTORELOGOPRUEBA__1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/DANGSTORELOGOPRUEBA__1.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "pan",
      "windowSoftInputMode": "adjustResize",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.anonymous.dangstore"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "La app necesita acceso a las fotos para guardar imágenes de encargos personalizados.",
          "savePhotosPermission": "La app necesita permisos para guardar fotos en la galería.",
          "isAccessMediaLocationEnabled": true,
          "isAccessMediaLocationEnabled": true
        }
      ]
    ]
  }
}
